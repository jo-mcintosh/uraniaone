  element taxonomy {
    (((tei_category | tei_taxonomy)+
      | ((tei_model.descLike | tei_equiv | tei_gloss)+,
         (tei_category | tei_taxonomy)*))
     | (tei_model.biblLike, (tei_category | tei_taxonomy)*)),
