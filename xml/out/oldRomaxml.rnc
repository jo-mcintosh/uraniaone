    [ a:defaultValue = "http://www.tei-c.org/ns/1.0 teix:egXML" ]
    attribute defaultExceptions {
      list {
        (xsd:anyURI { pattern = "[^/\s]+:\S*" }
         | xsd:Name { pattern = ".+:.+" })+
      }
