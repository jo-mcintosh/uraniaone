    tei_att.canonical.attributes,
    tei_att.typed.attributes,
    (
     ## indicates one or more systems or calendars to which the date represented by the content of this element belongs.
     attribute calendar {
       list {
         xsd:anyURI { pattern = "\S+" }+
       }
