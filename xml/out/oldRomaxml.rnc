    >> sch:pattern [
         id =
           "myTEI-unitDef-calendar-calendar-check-unitDef-constraint-rule-27"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:*[@calendar]"
           "\x{a}" ~
           "               "
           sch:assert [
