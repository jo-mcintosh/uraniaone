tei_valItem =
  
  ## documents a single value in a predefined list of values. [22.5.3. Attribute List Specification]
  element valItem {
    (tei_model.identSynonyms*,
     (tei_model.descLike+,
      (tei_model.identSynonyms, tei_model.descLike*)*,
      tei_remarks*)?,
     tei_paramList?),
