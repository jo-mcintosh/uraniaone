     tei_classes?,
     tei_constraintSpec*,
     tei_attList?,
     tei_exemplum*,
     tei_remarks*,
     tei_listRef*),
    tei_att.global.attributes,
    tei_att.identified.attributes,
    tei_att.typed.attribute.subtype,
    
    ## indicates whether this is a model class or an attribute class
