      ns1:code [ "<surface>" ]
      " element. [11.1. Digital Facsimiles 11.2.2. Embedded Transcription]"
    ]
  ]
  element zone {
    (text
     | tei_model.gLike
     | tei_model.graphicLike
     | tei_model.global
     | tei_surface
     | tei_model.linePart)*,
